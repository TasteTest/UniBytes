# PostgreSQL Connection String (Docker Defaults)
# Option 1: Full connection string
ConnectionStrings__PaymentConnection=Host=localhost;Port=5432;Database=payments_db;Username=payments_user;Password=payments_pass

# Option 2: Individual environment variables
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=payments_db
POSTGRES_USER=payments_user
POSTGRES_PASSWORD=payments_pass

# Stripe API Keys
# Get your keys from https://dashboard.stripe.com/test/apikeys
Stripe__PublishableKey=pk_test_your_publishable_key
Stripe__SecretKey=sk_test_your_secret_key
Stripe__WebhookSecret=whsec_your_webhook_secret

